{
    "name": "dach",
    "version": "1.0.0",
    "description": "",
    "author": "Maciej Kukielka (7sne)",
    "license": "MIT",
    "main": "./dist/cli.cjs",
    "bin": {
        "dach": "dist/cli.cjs"
    },
    "exports": {
        ".": {
            "require": "./dist/cli.cjs",
            "import": "./dist/cli.mjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm unbuild",
        "release": "vitest run && changelogen --release && git push --follow-tags && npm publish",
        "stub": "pnpm unbuild --stub",
        "lint": "eslint --ext .ts,.js . && prettier src/**/*.ts test/**/*.ts",
        "lint:fix": "eslint --fix --ext .ts,.js . && prettier --write src/**/*.ts test/**/*.ts",
        "test": "vitest run --coverage",
        "dev": "cross-env esno src/cli.ts",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "canvas": "^2.11.2",
        "commander": "^11.0.0",
        "yoga-layout-prebuilt": "^1.10.0",
        "zod": "^3.22.2"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.39.5",
        "@types/node": "^20.4.8",
        "@types/pixelmatch": "^5.2.4",
        "@types/pngjs": "^6.0.1",
        "@types/yoga-layout": "^1.9.4",
        "@vitest/coverage-v8": "^0.34.4",
        "cross-env": "^7.0.3",
        "eslint": "^8.43.0",
        "execa": "^8.0.1",
        "pixelmatch": "^5.3.0",
        "pngjs": "^7.0.0",
        "prettier": "^3.0.3",
        "typescript": "^5.1.5",
        "unbuild": "^1.2.1",
        "unconfig": "^0.3.9",
        "vitest": "^0.34.4"
    },
    "peerDependencies": {
        "typescript": "^5.2.2"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    },
    "eslintConfig": {
        "extends": "@antfu"
    },
    "packageManager": "pnpm@8.6.7"
}
